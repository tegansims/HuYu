<div class="login-form-container">
<%= form_tag '/login' do %>
    <%= label_tag :username, "Username", {class: "col-sm-2 col-form-label"} %>
    <%= text_field_tag :username, nil, class: "form-control-plaintext" %>
    <%= label_tag :password, "Password", class: "col-sm-2 col-form-label" %>
    <%= password_field_tag :password, nil, class: "form-control-plaintext" %>
    <%= submit_tag 'Login', class: "btn btn-primary"%>
<% end %>
<%= link_to "Create Account?", new_user_path %>



</div>
<br>
<br>

<%= render '/users/analytics' %>